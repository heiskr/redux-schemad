{
  "name": "redux-schemad",
  "version": "0.1.0",
  "description": "Create automatic reducers, actions, and default state from a state schema.",
  "keywords": [
    "redux",
    "schema",
    "state-schema",
    "automatic-reducers",
    "redux-schemad"
  ],
  "homepage": "https://github.com/heiskr/redux-schemad",
  "bugs": {
    "url": "https://github.com/heiskr/redux-schemad/issues"
  },
  "license": "Apache-2.0",
  "author": "Kevin Heis",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/heiskr/redux-schemad.git"
  },
  "scripts": {
    "covreport": "open coverage/lcov-report/index.html",
    "precommit": "pretty-quick --staged",
    "lint": "eslint index.js test.js",
    "test": "concurrently 'jest test.js' 'npm run lint'"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "devDependencies": {
    "concurrently": "3.5.1",
    "eslint": "4.18.2",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.9.0",
    "jest": "22.4.2",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1"
  },
  "engines": {
    "node": ">=8.9.4"
  }
}
